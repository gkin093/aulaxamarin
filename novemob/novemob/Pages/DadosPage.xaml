<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="novemob.DadosPage"
		BackgroundColor="Gray" Title="Dados">
	<ContentPage.Content>
		<StackLayout VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand">
			
			<Entry x:Name="txtNome" Placeholder="Nome" WidthRequest="300" />

			<Entry x:Name="txtSobrenome" Placeholder="Sobrenome" WidthRequest="300" />

			<Entry x:Name="txtEmail" Placeholder="Email" Keyboard="Email" WidthRequest="300" />

			<Entry x:Name="txtSenha" Placeholder="Senha" IsPassword="true" WidthRequest="300" />

			<Entry x:Name="txtConfSenha" Placeholder="Confirmação da Senha" WidthRequest="300" />
		</StackLayout>
	</ContentPage.Content>
</ContentPage>
